{
  "name": "headlessui-float",
  "version": "0.0.0",
  "description": "Eazy use Headless UI components with Floating UI (Popper.js)",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/headlessui-float-react",
    "packages/headlessui-float-vue",
    "examples/example-react",
    "examples/example-vue"
  ],
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:react": "yarn react build",
    "build:vue": "yarn vue build",
    "watch": "npm-run-all -p watch:**",
    "watch:react": "yarn react watch",
    "watch:vue": "yarn vue watch",
    "watch:example:react": "yarn react:example dev --port=3010",
    "watch:example:vue": "yarn vue:example dev --port=3011",
    "react": "yarn workspace headlessui-float-react",
    "react:example": "yarn workspace headlessui-float-example-react",
    "vue": "yarn workspace headlessui-float-vue",
    "vue:example": "yarn workspace headlessui-float-example-vue"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
