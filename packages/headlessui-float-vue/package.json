{
  "name": "headlessui-float-vue",
  "version": "0.0.0",
  "description": "Eazy use Headless UI for Vue 3 with Floating UI (Popper.js)",
  "license": "MIT",
  "keywords": [
    "floating",
    "popper",
    "popover",
    "tooltip",
    "dropdown",
    "headless",
    "headlessui",
    "vue"
  ],
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/headlessui-float.es.js",
  "module": "./dist/headlessui-float.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/headlessui-float.es.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/headlessui-float.git",
    "directory": "packages/headlessui-float-vue"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && vue-tsc --emitDeclarationOnly --outDir ./dist",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@floating-ui/core": "^0.5.0",
    "@floating-ui/dom": "^0.3.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.0",
    "lodash.throttle": "^4.1.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue": "^3.2.0",
    "vue-tsc": "^0.29.8"
  }
}
