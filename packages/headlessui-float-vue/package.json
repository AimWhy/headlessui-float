{
  "name": "headlessui-float-vue",
  "version": "0.5.2",
  "description": "Easy use Headless UI for Vue 3 with Floating UI (Popper.js)",
  "license": "MIT",
  "keywords": [
    "floating",
    "popper",
    "popover",
    "tooltip",
    "dropdown",
    "headless",
    "headlessui",
    "vue"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/headlessui-float.cjs.js",
  "module": "./dist/headlessui-float.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/headlessui-float.es.js",
      "require": "./dist/headlessui-float.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/headlessui-float.git",
    "directory": "packages/headlessui-float-vue"
  },
  "homepage": "https://github.com/ycs77/headlessui-float/tree/main/packages/headlessui-float-vue#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && vue-tsc --emitDeclarationOnly --outDir ./dist",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@floating-ui/core": "^0.6.2",
    "@floating-ui/dom": "^0.4.5"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.0",
    "lodash.throttle": "^4.1.1",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.9.5",
    "vue": "^3.2.0",
    "vue-tsc": "^0.34.7"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
