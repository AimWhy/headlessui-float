{
  "name": "headlessui-float-react",
  "version": "0.1.1",
  "description": "Eazy use Headless UI for React with Floating UI (Popper.js)",
  "license": "MIT",
  "keywords": [
    "floating",
    "popper",
    "popover",
    "tooltip",
    "dropdown",
    "headless",
    "headlessui",
    "react"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/headlessui-float.cjs.js",
  "module": "./dist/headlessui-float.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/headlessui-float.es.js",
      "require": "./dist/headlessui-float.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/headlessui-float.git",
    "directory": "packages/headlessui-float-react"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build && tsc --emitDeclarationOnly --outDir ./dist",
    "watch": "vite build --watch"
  },
  "devDependencies": {
    "@floating-ui/core": "^0.5.0",
    "@floating-ui/dom": "^0.3.1",
    "@floating-ui/react-dom": "^0.5.0",
    "@headlessui/react": "^1.5.0",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "lodash.throttle": "^4.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.0.0",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  }
}
